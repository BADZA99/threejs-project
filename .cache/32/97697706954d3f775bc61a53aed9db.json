lor: #6cff8c7a;\r\n$red-Color:#f44336;\r\n$blue-Color:#2196f3;\r\n$yellow-Color:#ffea5c;\r\n$dark-Color:#060b11;\r\n$darkish-blue:#060b11;\r\n$box-shadow-blue1:rgba(31,38,135,0.27);\r\n$box-shadow-blue2:rgba(31,38,135,0.67);\r\n\r\n\r\n// predefined classes\r\n\r\n.section{\r\n    padding: 1rem;\r\n    overflow: hidden;\r\n    min-height: 100vh;\r\n    padding-top: 5rem;\r\n\r\n    @media (min-width:650px){\r\n        padding: 5rem;\r\n    }\r\n\r\n    &__header{\r\n        &-title{\r\n            font-weight: 400;\r\n            font-size: 17px;\r\n            color: #666;\r\n        }\r\n        &-subtitle{\r\n            font-size: 27px;\r\n            font-weight: 700;\r\n        }\r\n    }\r\n}\r\n\r\n","assets\\styles\\loader.scss":"@import './base.scss';\r\n\r\n.loading {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    bottom: 0;\r\n    right: 0;\r\n    height: 100vh;\r\n    width: 100%;\r\n    z-index: 99;\r\n    background-color: #0b134f;\r\n    display: grid;\r\n    place-items: center;\r\n    font-family: \"Orbitron\", sans-serif;\r\n    // display: none;\r\n\r\n    &__box {\r\n        position: relative;\r\n        width: 500px;\r\n        height: 200px;\r\n        border: 3px solid $green-color;\r\n        border-top: 3px solid $dark-green-color;\r\n        border-bottom: 3px solid $dark-green-color;\r\n\r\n        @media (max-width:600px) {\r\n            & {\r\n                width: 90vw;\r\n            }\r\n\r\n        }\r\n    }\r\n\r\n    &__bar {\r\n        width: 90%;\r\n        height: 10px;\r\n        position: absolute;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translateX(-50%);\r\n        background: #ccc;\r\n        border-radius: 2px;\r\n\r\n        &--inner {\r\n            height: 100%;\r\n            width: 0;\r\n            border-radius: 2px;\r\n            background: $green-color;\r\n        }\r\n    }\r\n\r\n    &__text {\r\n        position: relative;\r\n        color: #fff;\r\n        padding: 1rem;\r\n        font-size: 20px;\r\n        font-weight: 700;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n\r\n        @media (max-width:355px) {\r\n            & {\r\n                font-size: 17px;\r\n            }\r\n\r\n        }\r\n\r\n        &--dot {\r\n            width: 15px;\r\n            height: 15px;\r\n            margin: 0 3px;\r\n            border-radius: 50%;\r\n            background: #fff;\r\n            animation: pulse 1s infinite;\r\n\r\n            @keyframes pulse {\r\n                from {\r\n                    opacity: 0;\r\n                    color: $green-color;\r\n                }\r\n\r\n                to {\r\n                    opacity: 1;\r\n                }\r\n\r\n            }\r\n        }\r\n\r\n        &--border {\r\n            width: 85%;\r\n            height: 1px;\r\n            background: $dark-green-color;\r\n            position: absolute;\r\n            left: 50%;\r\n            bottom: 0;\r\n            transform: translateX(-50%);\r\n        }\r\n\r\n    }\r\n\r\n    &__counter {\r\n        position: absolute;\r\n        top: 70%;\r\n        left: 0;\r\n        color: #fff;\r\n        font-style: 20px;\r\n        font-weight: 700;\r\n        width: 100%;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        padding: 10px;\r\n\r\n        &--number {\r\n            color: $green-color;\r\n        }\r\n    }\r\n\r\n    &__width_animation {\r\n        animation: width_animation 2s forwards;\r\n\r\n        @keyframes width_animation {\r\n            0% {\r\n                width: 85%;\r\n            }\r\n\r\n            100% {\r\n                width: 280px;\r\n            }\r\n\r\n        }\r\n    }\r\n\r\n    &__svg {\r\n        position: absolute;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%, -50%);\r\n        width: 84%;\r\n        opacity: 0;\r\n    }\r\n}"},"line