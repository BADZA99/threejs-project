ft: 5rem;\r\n        bottom: -2rem;\r\n        display: flex;\r\n        flex-direction: column;\r\n        font-family: 'Josefin Sans Std', sans-serif;\r\n        font-size: 17px;\r\n    }\r\n}\r\n\r\n.swiper-slide{\r\n    filter: blur(2px) !important;\r\n    transform: scale(0.8) !important;\r\n    transition: all 0.2s;\r\n}\r\n\r\n.swiper-slide-active{\r\n    filter: blur(0) !important;\r\n    transform: scale(1) !important;\r\n}\r\n\r\n.swiper__tools{\r\n    position: relative;\r\n    display: flex;\r\n    flex-direction: column-reverse;\r\n    gap: 2rem;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    margin-top: 6rem;\r\n    padding: 0 4rem 0 2rem;\r\n    // border: 2px solid red;\r\n\r\n    @media (min-width:1056px){\r\n        &{\r\n            flex-direction: row;\r\n        }\r\n    }\r\n    @media (max-width:1056px){\r\n        &{\r\n            flex-direction: row;\r\n        }\r\n    }\r\n}\r\n\r\n.swiper__buttons{\r\n    display: flex;\r\n    button{\r\n        position: relative;\r\n        background: transparent;\r\n        padding: 1rem 3rem;\r\n        margin-right: 2rem;\r\n        border-radius: 50px;\r\n        border: 1px solid #fff;\r\n        user-select: none;\r\n\r\n        &:after{\r\n            display: none;\r\n        }\r\n\r\n        img{\r\n            filter: invert(100%);\r\n        }\r\n\r\n        &:first-of-type{\r\n            transform: rotate(180deg);\r\n        }\r\n\r\n        &:hover{\r\n            background: #ffffff21;\r\n        }\r\n    }\r\n}\r\n\r\n.swiper-pagination{\r\n    position: relative;\r\n    display: none;\r\n\r\n    @media (min-width:807px){\r\n        &{\r\n            display: block;\r\n        }\r\n    }\r\n}\r\n\r\n.swiper-pagination-bullet{\r\n    width: 35px;\r\n    height: 4px;\r\n    border-radius: 50px;\r\n    background: #fff;\r\n}"},"lineCount":null}},"error":null,"hash":"61fbb74fc8c71bb3afdd5b1682e8fd15","cacheData":{"env":{}}}